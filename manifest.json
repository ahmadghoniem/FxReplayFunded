{
  "manifest_version": 3,
  "name": "fxreplayFunded",
  "version": "1.0",

  "background": {
    "service_worker": "src/extension/background.js"
  },

  "content_scripts": [
    {
      "matches": ["https://*.fxreplay.com/*"],
      "js": ["src/extension/utils.js", "src/extension/content.js"]
    }
  ],
  "action": {},
  "permissions": ["storage", "tabs"],
  "web_accessible_resources": [
    {
      "resources": ["dist/index.html", "dist/*"],
      "matches": ["<all_urls>"]
    }
  ]
}
