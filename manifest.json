{
  "manifest_version": 3,
  "name": "fxreplayFunded",
  "version": "1.0",

  "background": {
    "service_worker": "src/background.js"
  },

  "content_scripts": [
    {
      "matches": ["https://*.fxreplay.com/*"],
      "js": ["src/utils.js", "src/content.js"]
    }
  ],
  "action": {
    "default_popup": "dist/index.html"
  },
  "permissions": [
    "storage"
  ]
}
